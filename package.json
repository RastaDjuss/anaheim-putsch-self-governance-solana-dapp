{
  "name": "anaheim-putsch-govdapp",
  "version": "0.0.1.666",
  "private": true,
  "description": "Next.js, Tailwind, gill (based on @solana/kit), Wallet UI",
  "keywords": [
    "gill",
    "nextjs",
    "react",
    "solana-kit",
    "tailwind",
    "typescript",
    "wallet-ui"
  ],
  "type": "module",
  "packageManager": "pnpm@10.14.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "clean": "rm -rf .next dist out",
    "anchor:build": "cd anchor && cargo clean && cargo update && anchor build && cd .. && cp -r anchor/target/idl/* target/ && cp -r anchor/target/types/* target/ && pnpm build && pnpm start && pnpm keygen",
    "anchor:deploy": "cd anchor && anchor deploy --provider.cluster devnet && cd ..",
    "anchor:test": "cd anchor && anchor test && cd ..",
    "anchor:clean": "cd anchor && cargo clean && anchor clean && cargo update && anchor build && cd ..",
    "airdrop": "solana airdrop 2 && pnpm wallet && solana config get",
    "keygen": "solana-keygen new --outfile anchor/target/deploy/anarcrypt.json --force && solana config set --keypair anchor/target/deploy/anarcrypt.json && solana airdrop 2 && solana config get",
    "loot": "solana balance",
    "wallet:status": "solana balance && solana address && solana config get",
    "solana:config": "solana config set --keypair anchor/target/deploy/anarcrypt.json && solana config set --url devnet && solana airdrop 2 && solana address && solana balance && solana config get",
    "docker:build": "docker build -t anaheim:latest .",
    "docker:tag": "docker tag anaheim:latest anarcrypt/anaheim:latest",
    "docker:push": "docker push anarchsun/anaheim:latest",
    "docker:run": "docker run -p 3000:3000 anaheim:latest",
    "scaffold:init": "ts-node ./scripts/init-template.ts",
    "scaffold:fetch": "ts-node ./scripts/fetch-templates.ts",
    "sync": "pnpm anchor:build && pnpm lint",
    "putsch:dev": "pnpm airdrop && pnpm loot && pnpm dev",
    "putsch:deploy": "pnpm anchor:deploy && echo 'Deployed on devnet!'",
    "anchor:initialize": "tsx anchor/scripts/initialize.ts",
    "sync-idl": "mkdir -p src/lib/idl && cp anchor/target/idl/*.json src/lib/idl/"
  },
