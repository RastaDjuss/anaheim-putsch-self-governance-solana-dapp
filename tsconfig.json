{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "ESNext",
    "moduleResolution": "node",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "incremental": true,
    "jsx": "preserve",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "strictFunctionTypes": true,
    "baseUrl": ".",
    "paths": {
      "anchor/*": ["../../anchor/*"],
      "@/*": ["src/*"],
      "@coral-xyz/anchor": ["node_modules/@coral-xyz/anchor"],
      "@stake-lib/*": ["eco-subsystem/complementary-modules/getStakeActivation/js/src/*"],
      "@solana/web3.js": ["node_modules/@solana/web3.js"],
      "@anaheim/anchor-client": ["../../packages/anchor-client/src/index.ts"],
      "@vendor/*": ["vendor/*"]
    },
    "plugins": [{ "name": "next" }]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
